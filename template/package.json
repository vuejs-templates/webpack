    {{#lint}}
    "babel-eslint": "^6.1.2",
    {{/lint}}
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.24.0",
    {{#lint}}
